{
  "name": "@morpheme/alert",
  "version": "1.0.0-beta.5",
  "description": "Morpheme Alert - a flexible Vue alert component that can be used to display various types of messages to the user.",
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "prepublishOnly": "npm run build",
    "test": "vitest"
  },
  "keywords": [
    "alert",
    "ui-component",
    "ui",
    "vue",
    "morpheme"
  ],
  "author": "Warsono <warsono16694@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@morpheme/tailwind-config": "workspace:*",
    "@morpheme/button": "workspace:*",
    "@morpheme/icon": "workspace:*",
    "@morpheme/theme": "workspace:*",
    "@morpheme/utils": "workspace:*",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "c8": "^7.11.3",
    "postcss": "^8.4.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2",
    "vite": "^3.0.0",
    "vitest": "^0.12.4"
  },
  "gitHead": "92215d91d1e3a2b1b804e52b55dcfd0ed5274475",
  "main": "dist/alert.umd.js",
  "unpkg": "dist/alert.iife.js",
  "jsdelivr": "dist/alert.iife.js",
  "module": "./dist/alert.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/alert.mjs",
      "require": "./dist/alert.js"
    },
    "./package.json": "./package.json",
    "./src/*": "./src/*",
    "./*": "./*"
  }
}
